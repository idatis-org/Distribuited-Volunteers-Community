{
  "name": "volunteers-community",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/volunteers-community/src",
  "prefix": "test",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/volunteers-community",
        "index": "apps/volunteers-community/src/index.html",
        "main": "apps/volunteers-community/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/volunteers-community/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/volunteers-community/src/favicon.ico",
          "apps/volunteers-community/src/assets",
          {
            "glob": "**/*",
            "input": "apps/register/src/assets",
            "output": "assets"
          },
          {
            "glob": "**/*",
            "input": "dist/libs/idatis-ui/dist/idatis-ui/assets",
            "output": "assets"
          }
        ],
        "styles": [
          "apps/volunteers-community/src/styles.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/volunteers-community/webpack.config.js"
        },
        "commonChunk": false
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/volunteers-community/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "volunteers-community:build:production"
        },
        "development": {
          "browserTarget": "volunteers-community:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200"
      }
    },
    "extract-i18n": {
      "executor": "ngx-build-plus:extract-i18n",
      "options": {
        "browserTarget": "volunteers-community:build",
        "extraWebpackConfig": "apps/volunteers-community/webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/volunteers-community/**/*.ts",
          "apps/volunteers-community/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/volunteers-community/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
